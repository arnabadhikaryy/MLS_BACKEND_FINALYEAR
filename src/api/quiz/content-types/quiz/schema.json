{
  "kind": "collectionType",
  "collectionName": "quizzes",
  "info": {
    "singularName": "quiz",
    "pluralName": "quizzes",
    "displayName": "Quiz"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "blocks"
    },
    "lessons": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::lesson.lesson",
      "mappedBy": "quiz"
    },
    "questions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::question.question",
      "mappedBy": "quiz"
    },
    "quiz_attempts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::quiz-attempt.quiz-attempt",
      "mappedBy": "quiz"
    },
    "minutes": {
      "type": "integer"
    }
  }
}
